generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "mysql"
    url                  = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}

enum ServerLocation {
    BR
    EU
    NA
}

model Tag {
    name       String            @id
    characters CharacterObject[]
}

model CharacterObject {
    id   Int   @id
    tags Tag[]
}
